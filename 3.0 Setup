# name
name ruda

# mixer
mixer CUSTOM

mmix reset

mmix 0  1.000 -1.000  1.000 -1.000
mmix 1  1.000 -1.000 -1.000  1.000
mmix 2  1.000  1.000 -1.000 -1.000
mmix 3  1.000  1.000  1.000  1.000

# servo

# servo mix


# feature
feature -RX_PPM
feature RX_SERIAL
feature SOFTSERIAL
feature TELEMETRY
feature CURRENT_METER
feature AIRMODE

# beeper

# map
map TAER1234

# serial
serial 0 0 115200 57600 0 115200
serial 1 68 115200 57600 0 115200

# led

# color

# mode_color

# aux
aux 0 0 0 900 1100
aux 1 26 0 1825 2100
aux 2 29 0 900 1100

# adjrange
adjrange 0 0 2 900 1100 4 2
adjrange 1 1 2 1400 1600 4 2

# rxrange

# rxfail

# master
set roll_yaw_cam_mix_degrees = 30
set min_throttle = 1020
set motor_pwm_protocol = MULTISHOT
set serialrx_provider = SBUS
set deadband = 1
set failsafe_kill_switch = ON
